<include>
    <gateway name="{{ freeswitch_trunk_name }}">
	<param name="username" value="{{ freeswitch_trunk_username }}" />
	<param name="password" value="{{ freeswitch_trunk_password }}" />
	<param name="realm" value="{{ freeswitch_trunk_proxy }}" />
	<param name="proxy" value="{{ freeswitch_trunk_realm }}" />
	<param name="register" value="{{ freeswitch_trunk_register }}" />
	<param name="caller-id-in-from" value="true" />
	<param name="expire-seconds" value="{{ freeswitch_trunk_expire }}" />
    </gateway>
</include>

